# Teste Form #


public partial class FormLogin : Form
    {
        public FormLogin()
        {
            InitializeComponent();
        }

        private void BtEntrar_Click(object sender, EventArgs e)
        {
            if ((TextUsuario.Text == "admin") && (TextSenha.Text == "admin"))
            {
                FormPrincipal fp = new FormPrincipal();
                fp.Show();
                this.Hide();
            }
            else
            {
                MessageBox.Show("Usuário e senha não conferem", "Falha no Login", MessageBoxButtons.OK, MessageBoxIcon.Information);
            }

        }

    }
}
